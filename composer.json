{
    "name": "adminui/fakerphp-picsum-images",
    "description": "Alternative image provider for fakerphp using picsum.photos ",
    "keywords": [
        "faker",
        "php",
        "images",
        "picsum"
    ],
    "homepage": "https://github.com/adminui/fakerphp-picsum-images",
    "license": "MIT",
    "authors": [
        {
            "name": "Arnaud Becher",
            "email": "arnaud.becher@gmail.com",
            "role": "Developer"
        },
		{
            "name": "Craig Riley",
            "email": "c.riley@dsmdesign.co.uk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "fakerphp/faker": "^1.10"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpunit/phpunit": "^9.5",
        "spatie/ray": "^1.28"
    },
    "suggest": {
        "ext-curl": "*"
    },
    "autoload": {
        "psr-4": {
            "AdminUI\\FakerPicsumImages\\": "src"
        }
    },
    "scripts": {
    },
    "config": {
